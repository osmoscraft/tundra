{
  "keyBindings": [
    {
      "name": "Move line up",
      "run": "editor.moveLineUp",
      "key": "Alt-k"
    },
    {
      "name": "Move line down",
      "run": "editor.moveLineDown",
      "key": "Alt-j"
    },
    {
      "name": "Move cursor to block start",
      "run": "editor.moveCursorBlockStart",
      "key": "PageUp"
    },
    {
      "name": "Move cursor to block end",
      "run": "editor.moveCursorBlockEnd",
      "key": "PageDown"
    },
    {
      "name": "Select cursor to block start",
      "run": "editor.selectCursorBlockStart",
      "key": "Shift-PageUp"
    },
    {
      "name": "Select cursor to block end",
      "run": "editor.selectCursorBlockEnd",
      "key": "Shift-PageDown"
    },
    {
      "name": "Copy line up",
      "run": "editor.copyLineUp",
      "key": "Alt-Shift-k"
    },
    {
      "name": "Copy line down",
      "run": "editor.copyLineDown",
      "key": "Alt-Shift-j"
    },
    {
      "name": "Indent less",
      "run": "editor.indentLess",
      "key": "Alt-h"
    },
    {
      "name": "Indent more",
      "run": "editor.indentMore",
      "key": "Alt-l"
    },
    {
      "name": "Undo",
      "run": "editor.undo",
      "key": "Mod-z"
    },
    {
      "name": "Redo",
      "run": "editor.redo",
      "key": "Mod-Z"
    },
    {
      "name": "Options",
      "run": "shell.openOptions"
    },
    {
      "name": "Add link",
      "run": "shell.addLink",
      "key": "Mod-k"
    },
    {
      "name": "Search",
      "run": "shell.startSearch",
      "key": "Mod-p"
    },
    {
      "name": "Command",
      "run": "shell.startCommand",
      "key": "Mod-Shift-p"
    },
    {
      "name": "Save",
      "run": "file.save",
      "key": "Mod-s"
    },
    {
      "name": "Fetch",
      "run": "repo.fetch"
    },
    {
      "name": "New",
      "run": "file.new"
    },
    {
      "name": "Pull",
      "run": "repo.pull"
    },
    {
      "name": "Push",
      "run": "repo.push"
    },
    {
      "name": "Merge",
      "run": "repo.merge"
    },
    {
      "name": "Delete",
      "run": "file.delete"
    },
    {
      "name": "Resolve",
      "run": "repo.resolve"
    },
    {
      "name": "Sync",
      "run": "repo.sync",
      "key": "Mod-Shift-s"
    }
  ]
}
