      "run": "copyLineUp",
      "key": "Alt-Shift-k"
    },
    {
      "name": "Copy line down",
      "run": "copyLineDown",
      "key": "Alt-Shift-j"
    },
    {
      "name": "Indent less",
      "run": "indentLess",
      "key": "Alt-h"
    },
    {
      "name": "Indent more",
      "run": "indentMore",
      "key": "Alt-l"
    },
    {
      "name": "Undo",
      "run": "undo",
      "key": "Mod-z"
    },
    {
      "name": "Redo",
      "run": "redo",
      "key": "Mod-Z"
    },
    {
      "name": "Options",
      "run": "openOptions"
    },
    {
      "name": "Add link",
      "run": "openOmniboxLinkMode",
