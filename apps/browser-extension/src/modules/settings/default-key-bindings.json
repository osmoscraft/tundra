    {
      "name": "New",
      "run": "newFile"
    },
    {
      "name": "Pull",
      "run": "pullChanges"
    },
    {
      "name": "Push",
      "run": "pushChanges"
    },
    {
      "name": "Merge",
      "run": "mergeChanges"
    },
    {
      "name": "Delete",
      "run": "deleteFile"
    },
    {
      "name": "Resolve",
      "run": "resolveConflicts"
    },
    {
      "name": "Sync",
      "run": "syncChanges"
    },
    {
      "name": "Save and sync",
      "run": "saveFilesAndSyncChanges",
      "key": "Mod-Shift-s"
    },
    {
