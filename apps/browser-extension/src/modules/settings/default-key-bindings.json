{
  "keyBindings": [
    {
      "name": "Open search panel",
      "run": "openSearchPanel",
      "key": "Mod-f"
    },
    {
      "name": "Close search panel",
      "run": "closeSearchPanel",
      "key": "Escape",
      "scope": "search-panel"
    },
    {
      "name": "Move line up",
      "run": "moveLineUp",
      "key": "Alt-k"
    },
    {
      "name": "Move line down",
      "run": "moveLineDown",
      "key": "Alt-j"
    },
    {
      "name": "Move cursor to block start",
      "run": "moveCursorBlockStart",
      "key": "PageUp"
    },
    {
      "name": "Move cursor to block end",
      "run": "moveCursorBlockEnd",
      "key": "PageDown"
    },
    {
      "name": "Select cursor to block start",
      "run": "selectCursorBlockStart",
      "key": "Shift-PageUp"
    },
    {
      "name": "Select cursor to block end",
      "run": "selectCursorBlockEnd",
      "key": "Shift-PageDown"
    },
    {
