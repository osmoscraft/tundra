{
  "keyBindings": [
    {
      "name": "Move line up",
      "run": "editor.moveLineUp",
      "key": "Alt-ArrowUp"
    },
    {
      "name": "Move line down",
      "run": "editor.moveLineDown",
      "key": "Alt-ArrowDown"
    },
    {
      "name": "Copy line up",
      "run": "editor.copyLineUp",
      "key": "Alt-Shift-ArrowUp"
    },
    {
      "name": "Copy line down",
      "run": "editor.copyLineDown",
      "key": "Alt-Shift-ArrowDown"
    },
    {
      "name": "Indent less",
      "run": "editor.indentLess",
      "key": "Alt-ArrowLeft"
    },
    {
      "name": "Indent more",
      "run": "editor.indentMore",
      "key": "Alt-ArrowRight"
    },
    {
      "name": "Undo",
      "run": "editor.undo",
      "key": "Mod-z"
    },
    {
      "name": "Redo",
      "run": "editor.redo",
      "key": "Mod-Z"
    },
    {
      "name": "Options",
      "run": "shell.openOptions"
    },
    {
      "name": "Add link",
      "run": "shell.addLink",
      "key": "Mod-k"
    },
    {
      "name": "Search",
      "run": "shell.startSearch",
      "key": "Mod-p"
    },
    {
      "name": "Command",
      "run": "shell.startCommand",
      "key": "Mod-Shift-p"
    },
    {
      "name": "Save",
      "run": "file.save",
      "key": "Mod-s"
    },
    {
      "name": "New",
      "run": "file.new"
    },
    {
      "name": "Sync",
      "run": "repo.sync"
    },
    {
      "name": "Pull",
      "run": "repo.pull"
    },
    {
      "name": "Push",
      "run": "repo.push"
    },
    {
      "name": "Fetch",
      "run": "repo.fetch"
    }
  ]
}
